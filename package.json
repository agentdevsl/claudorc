{
  "name": "agent-dev-template",
  "version": "1.0.0",
  "description": "Framework-agnostic TypeScript/Node.js web development template with AI-assisted workflows",
  "type": "module",
  "engines": {
    "node": ">=24.0.0"
  },
  "scripts": {
    "dev": "bun scripts/start-dev.ts",
    "dev:simple": "bun run dev:api & vite",
    "dev:vite": "vite",
    "dev:api": "bun src/server/api.ts",
    "build": "vite build && tsc --noEmit && npm run build:agent-runner",
    "build:agent-runner": "cd agent-runner && npm run build",
    "lint": "biome lint .",
    "lint:fix": "biome lint --write .",
    "format": "biome format --write .",
    "format:check": "biome format .",
    "check": "biome check .",
    "check:fix": "biome check --write .",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "vitest --config vitest.e2e.config.ts",
    "test:ui": "bun scripts/run-ui-tests.ts",
    "test:integration": "vitest --include 'tests/integration/**'",
    "test:k8s": "K8S_INTEGRATION_TESTS=true vitest run --include 'src/lib/sandbox/providers/__tests__/k8s-tmux.integration.test.ts'",
    "typecheck": "tsc --noEmit",
    "db:generate": "drizzle-kit generate",
    "db:push": "drizzle-kit push",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio",
    "prepare": "echo 'Run: pre-commit install --hook-type pre-commit --hook-type pre-push'"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.11",
    "@playwright/test": "^1.58.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/better-sqlite3": "^7.6.12",
    "@types/dockerode": "^4.0.0",
    "@types/node": "^25.0.9",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitest/coverage-v8": "^4.0.16",
    "agent-browser": "0.7.6",
    "drizzle-kit": "^0.31.8",
    "jsdom": "^27.4.0",
    "playwright": "^1.57.0",
    "typescript": "^5.7.2",
    "vitest": "^4.0.16"
  },
  "keywords": [
    "template",
    "typescript",
    "nodejs",
    "vitest",
    "eslint",
    "ai-assisted"
  ],
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/claude-agent-sdk": "^0.2.29",
    "@anthropic-ai/sdk": "^0.72.1",
    "@cdktf/hcl2json": "^0.21.0",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@durable-streams/client": "^0.2.1",
    "@durable-streams/server": "^0.2.0",
    "@durable-streams/state": "^0.2.0",
    "@kubernetes/client-node": "^1.4.0",
    "@paralleldrive/cuid2": "^3.0.6",
    "@phosphor-icons/react": "^2.1.10",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.15",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tailwindcss/vite": "^4.1.18",
    "@tanstack/db": "^0.5.20",
    "@tanstack/react-db": "^0.1.64",
    "@tanstack/react-router": "^1.150.0",
    "@tanstack/react-start": "^1.150.0",
    "@tanstack/router-plugin": "^1.157.17",
    "@vitejs/plugin-react": "^5.1.2",
    "@xyflow/react": "12.10.0",
    "better-sqlite3": "^12.6.2",
    "class-variance-authority": "^0.7.1",
    "dockerode": "^4.0.9",
    "drizzle-orm": "^0.45.1",
    "elkjs": "^0.11.0",
    "hono": "^4.11.5",
    "octokit": "^5.0.5",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-markdown": "^10.1.0",
    "shiki": "^3.22.0",
    "tailwindcss": "^4.1.18",
    "vite": "^7.3.1",
    "vite-tsconfig-paths": "^6.0.5",
    "zod": "4.3.6"
  }
}
